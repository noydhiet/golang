pipeline {
        agent any
        stages{
               stage('Checkout') {
                     steps{
                        git url: 'https://github.com/noydhiet/golang.git'
                     }
               }
               stage('preTest') {
                      steps{
                        go version
                        go get -u github.com/noydhiet/golang.git
                        dep init
                      }
               }
               stage('Build'){
                      steps{
                        go build
                      }                     
               }
        }
}
